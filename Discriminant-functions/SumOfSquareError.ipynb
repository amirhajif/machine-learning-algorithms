   "source": [
    "import numpy as np\n",
    "from sklearn.metrics import mean_squared_error\n",
    "\n",
    "y_actual = np.array([3, -0.5, 2, 7])\n",
    "y_pred = np.array([2.5, 0.0, 2, 8])\n",
    "\n",
    "sse = np.sum((y_actual - y_pred)**2)\n",
    "print(\"SSE:\", sse)\n",
    "\n",
    "mse = mean_squared_error(y_actual, y_pred)\n",
    "print(\"MSE:\", mse)"
   ]
  }
